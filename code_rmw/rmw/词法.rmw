>
  ./态
    *:态
  ./词法/小函数
    行缩进
  ./词法/导入

< (源码)=>
  源码 = 源码.replaceAll('\r\n','\n').replaceAll('\r','\n').split('\n')

  = 行 = 0
    列 = 0
    行列

  - 源码行数 = 源码.length
    果 = []
    返 = 果.push.bind 果

  while(源码行数>行){
    行列 = 导入(返,源码,行,列)
    if(Array.isArray(行列)){
      [行,列] = 行列
    }else{
      if(Number.isInteger(行列)){
        if(行列>行){
          列 = 0
          行 = 行列
          continue
        }
      }
    }

    ++行
  }
  < 果
